local args = {
    [1] = "Boombox",
    [2] = "5890236784"
}

game:GetService("ReplicatedStorage"):FindFirstChild(".gg/untitledhood"):FireServer(unpack(args))
wait(3.3)
-- Script generated by SimpleSpy - credits to exx#9394

