messageLL = "Thanks For Using BalliPrem" 


---------------------commands--------------------------------------------------------------------------
--bring teleports alts to you
--msg says message
--sit sits alts
--unsit unsits alts
--bighead make your alt head bigger  
--float floats alts
--unfloat unfloats alts
--say makes alts say message
--noclip phase trugh walls (r6)
--unclip no noclip (r6)
--giveanim give your alt freeze animation
--wallet show your alt wallet
--loopjump make your alt autojump
--drop make your alt drop money in da hood (1000)
--skinny make your alt body skinny (required skinny body)
--headless make your alt headless
--freeze make your alt freeze
--unfreeze make your alt unfreeze
--kill kill your alt
--kick kick your alt
--stand make your alt be your stand
--benx make your alt benx
--walk make your alt walk


