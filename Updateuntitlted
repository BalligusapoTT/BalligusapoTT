                if v:IsA('Part') and v.Name == "MoneyDrop" then
                    repeat
                        pcall(function()
                            chr.HumanoidRootPart.CFrame = CFrame.new(v.Position)
                        end)
                        fireclickdetector(v:WaitForChild('ClickDetector'))
                        task.wait()
                    until not v:IsDescendantOf(workspace.Ignored.Drop)
                end
            end
end)
end)
